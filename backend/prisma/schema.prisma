generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Exercise {
  id          Int     @id @default(autoincrement())
  title       String
  description String
  algorithm   String
}
